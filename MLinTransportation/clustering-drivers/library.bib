journal = {IEEE Transactions on Intelligent Transportation Systems},
keywords = {Crash-imminent braking,intelligent transportation systems (ITSs),precollision system (PCS),rear-end collisions,safety systems},
number = {4},
pages = {1546--1555},
publisher = {IEEE},
title = {{Safety benefits of forward collision warning, brake assist, and autonomous braking systems in rear-end collisions}},
