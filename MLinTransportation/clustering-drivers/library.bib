journal = {Accident Analysis and Prevention},
keywords = {Attention Networks Test,Distraction,Driver behaviour,Driving simulator,Hazardous situations,Inattention},
pages = {1193--1206},
pmid = {23084094},
title = {{The influence of differences in the functioning of the neurocognitive attentional networks on drivers' performance}},
volume = {50},
year = {2013}
}
