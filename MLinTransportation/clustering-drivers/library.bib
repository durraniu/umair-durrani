@inproceedings{Durrani2022,
abstract = {This study predicted the driver's reaction time in approaching and braking conditions using the Wiedemann car-following model and the Accumulator model. Data were collected from a driving simulator in two scenarios: 1) approach and follow a moving lead vehicle and 2) approach a stopped lead vehicle. The reaction times were predicted based on looming (angular velocity due to the width and height of lead vehicle, tau-inverse), lead vehicle type (car and truck), and lead vehicle brake lights (on or off). It was found that the the reaction time depends on looming in both approaching and braking conditions. The calibration of the Accumulator model shows that different looming variables better predicted the reaction time in different driving conditions. In general, the drivers were more sensitive to tau-inverse in urgent situations (approaching a stopped vehicle or braking before a moving vehicle) than angular velocity. The drivers were also more sensitive to angular velocity due to the height of lead truck than the width of lead truck. Moreover, the effect of lead vehicle brake lights on the reaction time was significant. The Accumulator model showed lower prediction errors of the reaction time than the Wiedeman model. In particular, the Wiedemann model predicted unrealistically long reaction times due to the assumption of constant looming threshold. The Accumulator model overcomes this limitation of the Wiedemann model by capturing a time lag in reactions as driving conditions change. Thus, the Accumulator model is a promising method of predicting the driver's reaction time in car-following situations.},
address = {Washington, D.C.},
author = {Durrani, Umair and Lee, Chris},
booktitle = {Presented at 101st Annual Meeting of Transportation Research Board},
title = {{Estimating Driver's Reaction Time Based on Looming in Approaching and Braking Conditions: Comparison of Wiedemann Car-Following Model and Accumulator Model}},
year = {2022}
}
