issn = {00494488},
journal = {Transportation},
keywords = {Density,Home-based work tour,Mode choice,New York Metropolitan Region,Residential self-selection},
number = {3},
pages = {285--299},
pmid = {1470202431},
title = {{Role of the built environment on mode choice decisions: Additional evidence on the impact of density}},
url = {https://www.researchgate.net/profile/Cynthia{\_}Chen3/publication/5159101{\_}Role{\_}of{\_}the{\_}built{\_}environment{\_}on{\_}mode{\_}choice{\_}decisions{\_}additional{\_}evidence{\_}on{\_}the{\_}impact{\_}of{\_}density/links/544f1acc0cf2bca5ce90c97c.pdf},
volume = {35},
