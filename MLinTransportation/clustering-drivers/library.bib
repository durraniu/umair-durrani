pages = {105889},
publisher = {Elsevier Ltd},
title = {{Predicting driver reaction time and deceleration: Comparison of perception-reaction thresholds and evidence accumulation framework}},
url = {https://doi.org/10.1016/j.aap.2020.105889},
volume = {149},
