doi = {10.1007/s11116-010-9309-6},
file = {:C$\backslash$:/Users/umair/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/van Acker, Witlox - 2011 - Commuting trips within tours How is commuting related to land use.pdf:pdf},
