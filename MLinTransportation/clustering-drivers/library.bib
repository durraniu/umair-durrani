@article{Berthaume2018,
abstract = {Microsimulation modelling is a tool used by practitioners and researchers to predict and evaluate the flow of traffic on real transportation networks. These models are used in practice to inform decisions and thus must reflect a high level of accuracy. Microsimulation models are comprised of sub-models, which control individual vehicle movements throughout the simulated network. These sub-models must be calibrated to accurately reflect realistic actions of drivers. This research utilizes data collected by the FHWA Living Laboratory instrumented research vehicle (IRV) to produce evidence of global trends in car-following behavior. Unlike similar studies, this analysis focuses on the physical action taken by the driver—the acceleration—rather than the outcome of that action—speed selection or time/distance gap. This approach enables better interpretation and comparison between car-following behavior on different functional classifications of roadways, operational conditions (i.e. work zone or non-work zone interstate segment), and traffic conditions. This analysis produces conclusive evidence that car-following behavior is heterogeneous and is a function of the prevailing driving environment—functional classification, operational condition, and traffic conditions of a roadway. Global trends in acceleration behaviors between different road types and traffic conditions are discussed and a statistical analysis identifies regions of significant driver acceleration behavior differences. Due to the nature of the applied IRV data, car-following behavior in work zones and non-work zones were compared and evidence of driving behavior differences—in terms of car-following acceleration behavior—was verified.},
author = {Berthaume, Andrew L. and James, Rachel M. and Hammit, Britton E. and Foreman, Christina and Melson, Christopher L.},
doi = {10.1177/0361198118798713},
file = {:C$\backslash$:/Users/umair/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Berthaume et al. - 2018 - Variations in Driver Behavior An Analysis of Car-Following Behavior Heterogeneity as a Function of Road Type a.pdf:pdf},
issn = {21694052},
journal = {Transportation Research Record},
number = {37},
pages = {31--44},
title = {{Variations in Driver Behavior: An Analysis of Car-Following Behavior Heterogeneity as a Function of Road Type and Traffic Condition}},
volume = {2672},
year = {2018}
}
