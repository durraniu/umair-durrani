keywords = {Calibration,Car-following,Lane-changing,NGSIM,Traffic oscillations,Validation},
month = {oct},
pages = {82--106},
title = {{Trajectory data reconstruction and simulation-based validation against macroscopic traffic patterns}},
url = {http://www.sciencedirect.com/science/article/pii/S0191261515001393},
volume = {80},
year = {2015}
}
