@inproceedings{Durrani2018,
abstract = {This study develops the method of estimating lane change duration (LCD) and analyzes the impact of surrounding trucks on car driver's LCD. Driver behavior data were collected from 50 drivers using a driving simulator. Drivers tested two scenarios – the scenario with surrounding cars only (Cars scenario) and the scenario with surrounding trucks only (Trucks scenario). Lateral positions of the subject and surrounding vehicles in 270 discretionary and mandatory lane changes were extracted from vehicle trajectories. The start and end times of each lane change were estimated using a constrained-slope piecewise regression model. It was found that the LCD was generally shorter for the Trucks scenario than the Cars scenario. This is potentially because car drivers are more likely to avoid conflicts with trucks by changing lanes faster. Linear regression models were also developed to identify the association of the LCD with driver and lane-change characteristics, vehicle dynamics and traffic kinematics at 4 different time frames - 0.5 s before start, start, middle (cross the lane marking) and end of lane change. The model results show that the LCD increases with the time gap in origin lane at the middle time frame only in the Cars scenario. However, the LCD was shorter when the time gap in origin lane was shorter, speed of subject vehicle was higher and accelerator pedal was pressed 0.5 s before start, start and middle time frames in the Trucks scenario. This study demonstrates that type of surrounding vehicles must be considered for estimation of the LCD.},
author = {Durrani, Umair and Lee, Chris},
booktitle = {Presentated at 97th Annual Meeting of Transportation Research Board},
title = {{Assessing Impacts of Surrounding Trucks on Car Drivers' Lane Change Duration Using Driving Simulator}},
year = {2018}
