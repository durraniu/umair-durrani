doi = {10.1016/j.aap.2014.06.008},
file = {:C$\backslash$:/Users/umair/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Lee, Li - 2014 - Analysis of injury severity of drivers involved in single- and two-vehicle crashes on highways in Ontario.pdf:pdf},
