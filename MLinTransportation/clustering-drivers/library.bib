journal = {Accident Analysis and Prevention},
keywords = {Data mining,Freeway crashes,Lane-change crashes,Loop detector data,Real-time traffic,Sideswipe collisions},
number = {5},
pages = {936--948},
pmid = {16729948},
title = {{Assessment of freeway traffic parameters leading to lane-change related collisions}},
volume = {38},
