issn = {1076898X},
journal = {Journal of Experimental Psychology: Applied},
keywords = {collision avoidance,deceleration,driving,optic flow,visual perception},
number = {4},
pages = {334--350},
pmid = {20025419},
title = {{Responses to Deceleration During Car Following: Roles of Optic Flow, Warnings, Expectations, and Interruptions}},
volume = {15},
