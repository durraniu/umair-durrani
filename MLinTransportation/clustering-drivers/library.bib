keywords = {Car-following,Distraction,Driver behaviour,Mobile phone use while driving,Risk compensation},
month = {may},
pages = {10--19},
pmid = {26009990},
title = {{Impact of mobile phone use on car-following behaviour of young drivers.}},
url = {http://www.sciencedirect.com/science/article/pii/S0001457515001773},
volume = {82},
year = {2015}
}
