journal = {Journal of Safety Research},
keywords = {Age,Braking time,Event uncertainty,Gender,Movement time,Reaction time,Transmission type},
month = {mar},
number = {1},
pages = {117--128},
title = {{Effects of uncertainty, transmission type, driver age and gender on brake reaction and movement time}},
url = {http://www.sciencedirect.com/science/article/pii/S0022437502000063},
volume = {33},
year = {2002}
}
